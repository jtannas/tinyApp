<h1>TinyApp</h1>
<% if (locals.user) {%>
  <form method="POST" action="/logout">
    <label><%= user.email %></label>
    <input type="Submit" value="logout">
  </form>
  <a class="button" href="/urls">My Urls</a>
<% } else {%>
  <a class="button" href="/login">Login</a>
  <a class="button" href="/register">Register</a>
<% } %>
