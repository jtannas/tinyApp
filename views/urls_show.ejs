<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>TinyApp</title>
</head>

<body>

  <header>
    <%- include ./partials/_header %>
  </header>

  <table>
    <thead>
      <tr>
        <th>Key</th>
        <th>Url</th>
      </tr>
    </thead>
    <tbody>
      <% for (key in urls) {%>
      <tr>
        <td><a href="http://<%= host %>/urls/<%= key %>">http://<%= host %>/urls/<%= key %></a></td>
        <td><a href="<%= urls[key] %>"><%= urls[key] %></a></td>
      </tr>
      <%}%>
    </tbody>
  </table>

</body>
</html>
